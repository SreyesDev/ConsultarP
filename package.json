{
  "name": "ConsultarP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios --simulator=\"iPhone 15\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",

    "// ═════════════════════════════════════════════════════════════": "",
    "// 🍎 iOS - DESARROLLO (solo macOS)": "",
    "// ════════════════════════════════════════════════════════════════": "",
    "ios-setup": "yarn run ios-uninstaller && yarn run ios-installer && yarn run refreshIOS",
    "ios-installer": "yarn install && cd ios && pod setup && pod install --repo-update && pod update",
    "ios-uninstaller": "watchman watch-del-all && yarn cache clean && rm -rf node_modules/ && rm -f yarn.lock && cd ios && rm -rf Pods Podfile.lock build assets main.jsbundle && pod deintegrate && rm -rf ~/Library/Caches/CocoaPods && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "ios-uninstaller-light": "yarn cache clean && rm -rf node_modules/ && rm -f yarn.lock && cd ios && rm -rf Pods Podfile.lock build assets main.jsbundle && pod deintegrate",
    "build:ios": "npx react-native bundle --entry-file ./index.js --bundle-output ios/main.jsbundle --dev=false --platform='ios' --assets-dest='./ios'",

    "// ════════════════════════════════════════════════════════════": "",
    "// 🤖 ANDROID - DESARROLLO": "",
    "// ═══════════════════════════════════════════════════════════": "",
    "android-build-qa": "cd android && ./gradlew clean assembleRelease",
    "android-build-prod": "cd android && ./gradlew clean bundleRelease",
    "build:dev": "cd android && ./gradlew assembleRelease",
    "clean": "yarn cache clean && cd ./android && ./gradlew clean",
  },
  "dependencies": {
    "@react-native-picker/picker": "github:react-native-picker/picker",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-picker-select": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
